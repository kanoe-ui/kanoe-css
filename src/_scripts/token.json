        "options": [
          {
            "name": "sans-serif-custom",
            "value": "'Inter', linkMacSystemFont, -apple-systemx, 'Segoe UI', Roboto, 'Helvetica Neuex', Helveticax, Arial, sans-serif",
            "default": true
          },
          {
            "name": "sans-serif",
            "value": "linkMacSystemFont, -apple-systemx, 'Segoe UI', Roboto, 'Helvetica Neuex', Helveticax, Arial, sans-serif",
            "default": false
          },
          {
            "name": "monospace",
            "value": "Consolas, 'Andale Mono', Monaco, 'Courier New', Courier, monospace",
            "default": false
          }
        ]
      },
      "size": {
        "name": "size",
        "options": [
          { "name": "xxxx-large", "value": "39.81px", "default": false },
          { "name": "xxx-large", "value": "33.18px", "default": false },
          { "name": "xx-large", "value": "27.65px", "default": false },
          { "name": "x-large", "value": "23.04px", "default": false },
          { "name": "large", "value": "19.20px", "default": false },
          { "name": "default", "value": "16.00px", "default": true },
          { "name": "small", "value": "13.33px", "default": false },
          { "name": "x-small", "value": "11.11px", "default": false },
          { "name": "xx-small", "value": "9.26px", "default": false }
