          { "name": "regular", "value": "400", "default": true },
          { "name": "light", "value": "300", "default": false },
          { "name": "extra-light", "value": "200", "default": false },
          { "name": "thin", "value": "100", "default": false },
          { "name": "bolder", "value": "bolder", "default": false },
          { "name": "lighter", "value": "lighter", "default": false }
